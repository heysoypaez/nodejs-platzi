{ 
	"name": "platzivideo",
	"version": 2,
	"builds": [{"src": "/index.js","use": "@now/node"}],
	"routes": [{"src": "/(.*)", "dest": "/index.js"}],
	"env": {
		"PORT" : "@port",               
		"DB_USER" : "@db_user",            
		"DB_PASSWORD" : "@db_password",        
		"DB_HOST" : "@db_host",            
		"DB_NAME" : "@db_name",            
		"DB_PORT" : "@db_port",            
		"DB_MYSQL_USER" : "@db_mysql_user",      
		"DB_MYSQL_PASSWORD" : "@db_mysql_password",  
		"DB_MYSQL_HOST" : "@db_mysql_host",      
		"DB_MYSQL_NAME" : "@db_mysql_name"
	}
}